#! /usr/bin/env node

console.log(require('figlet').textSync('<%= splashName %>', { font: 'Ogre' }));

const <%= serverInstanceName %> = require('../');


<%= serverInstanceName %>.start((err, result) => {
  if (err) { throw err; }
  <%= serverInstanceName %>.logger.info('Listening for HTTP requests at %s', result.url);
});
